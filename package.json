  "description": "Quiet.js",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "node_modules/path-exists-cli/cli.js src/libquiet/quiet.js || docker run -v ${PWD}/src/libquiet:/dist quiet-js-build",
